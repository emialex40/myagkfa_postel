<?xml version="1.0" encoding="utf-8"?>
<form cron="false">
	<fields name="jform">
		<fieldset name="icecat">
			<field
				name="ice_username"
				type="text"
				default="" />
			<field
				name="ice_password"
				type="password"
				default="" />
			<field
				name="ice_advanced"
				type="radio"
				class="btn-group btn-group-yesno"
				default="0">
				<option value="1">JYES</option>
				<option value="0">JNO</option>
			</field>
			<field
				name="ice_lang"
				type="list"
				class="input-xlarge"
				default="INT">
				<option value="INT">COM_CSVI_JFORM_ICE_INT</option>
				<option value="EN">COM_CSVI_JFORM_ICE_EN</option>
				<option value="US">COM_CSVI_JFORM_ICE_US</option>
				<option value="BG">COM_CSVI_JFORM_ICE_BG</option>
				<option value="CA">COM_CSVI_JFORM_ICE_CA</option>
				<option value="ZH">COM_CSVI_JFORM_ICE_ZH</option>
				<option value="HR">COM_CSVI_JFORM_ICE_HR</option>
				<option value="CZ">COM_CSVI_JFORM_ICE_CZ</option>
				<option value="DK">COM_CSVI_JFORM_ICE_DK</option>
				<option value="NL">COM_CSVI_JFORM_ICE_NL</option>
				<option value="FI">COM_CSVI_JFORM_ICE_FI</option>
				<option value="FR">COM_CSVI_JFORM_ICE_FR</option>
				<option value="KA">COM_CSVI_JFORM_ICE_KA</option>
				<option value="DE">COM_CSVI_JFORM_ICE_DE</option>
				<option value="HU">COM_CSVI_JFORM_ICE_HU</option>
				<option value="IT">COM_CSVI_JFORM_ICE_IT</option>
				<option value="JA">COM_CSVI_JFORM_ICE_JA</option>
				<option value="NO">COM_CSVI_JFORM_ICE_NO</option>
				<option value="PL">COM_CSVI_JFORM_ICE_PL</option>
				<option value="PT">COM_CSVI_JFORM_ICE_PT</option>
				<option value="RO">COM_CSVI_JFORM_ICE_RO</option>
				<option value="RU">COM_CSVI_JFORM_ICE_RU</option>
				<option value="SR">COM_CSVI_JFORM_ICE_SR</option>
				<option value="ES">COM_CSVI_JFORM_ICE_ES</option>
				<option value="SE">COM_CSVI_JFORM_ICE_SE</option>
				<option value="TR">COM_CSVI_JFORM_ICE_TR</option>
				<option value="UK">COM_CSVI_JFORM_ICE_UK</option>
			</field>
			<field
				name="ice_index"
				type="text"
				class="input-xxlarge"
				default="http://data.icecat.biz/export/freexml.int/INT/files.index.csv" />
			<field
				name="ice_supplier"
				type="text"
				class="input-xxlarge"
				default="http://data.icecat.biz/export/freexml.int/INT/supplier_mapping.xml" />
		</fieldset>
	</fields>
</form>
